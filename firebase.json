{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build\\default",
    "ignore": [
      "firebase.json",
      "**/.*"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
